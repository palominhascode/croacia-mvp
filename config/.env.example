# Croacia MVP - Configuration Example

# Serper API (Busca de URLs)
SERPER_API_KEY=your-serper-api-key-here

# Server Configuration
PORT=8080
BIND_ADDR=0.0.0.0
LOG_LEVEL=INFO

# Cache (se usar)
CACHE_ENABLED=true
CACHE_TTL=3600

# Rate Limiting
RATE_LIMIT_ENABLED=true
RATE_LIMIT_REQUESTS=100
RATE_LIMIT_WINDOW=60

# Features
ENABLE_CLOUDSCRAPER=true
ENABLE_CACHING=false
MAX_RESULTS=50

# Development
DEBUG=false
VERBOSE=false
